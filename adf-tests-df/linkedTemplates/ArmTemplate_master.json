{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-tests-df"
		},
		"KV_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://adf-tests-kv.vault.azure.net/"
		},
		"LS_ASQL_AdfTesting_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "AdfTestingDbConnectionString"
		},
		"LS_ASQL_ExternalSystem_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "ExternalSystemDbConnectionString"
		},
		"LS_ASQL_Parameterised_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "@linkedService().ConnectionStringSecretName"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-tests-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"KV_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('KV_AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"LS_ASQL_AdfTesting_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_ASQL_AdfTesting_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_ASQL_ExternalSystem_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_ASQL_ExternalSystem_properties_typeProperties_connectionString_secretName')]"
					},
					"LS_ASQL_Parameterised_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_ASQL_Parameterised_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}